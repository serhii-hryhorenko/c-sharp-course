<Window
    x:Class="Lab2.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Lab2 App">

    <StackPanel>
        <TextBlock Text="Ім'я" />
        <TextBox x:Name="FirstNameTextBox" />

        <TextBlock Text="Прізвище" />
        <TextBox x:Name="LastNameTextBox" />

        <TextBlock Text="Електронна адреса" />
        <TextBox x:Name="EmailTextBox" />

        <TextBlock Text="Дата народження" />
        <DatePicker x:Name="DateOfBirthPicker" />

        <Button x:Name="ProceedButton" Content="Proceed" IsEnabled="False" Click="OnProceedClicked" />
    </StackPanel>
</Window>
